    "type": "git",
    "url": "git+https://github.com/nightwatchjs/create-nightwatch"
  },
  "devDependencies": {
    "@npm/types": "^1.0.2",
    "@types/ejs": "^3.1.1",
    "@types/inquirer": "^8.2.1",
    "@types/minimist": "^1.2.2",
    "@types/mockery": "^1.4.30",
    "@types/nightwatch": "^2.3.11",
    "@types/node": "^17.0.41",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "mockery": "^2.1.0",
    "nightwatch": "^2.1.8",
    "nock": "^13.2.9",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
