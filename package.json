{
  "name": "create-nightwatch",
  "version": "1.2.4",
  "description": "A single-command tool to setup Nightwatch.js",
  "main": "index.js",
  "bin": {
    "create-nightwatch": "./index.js"
  },
  "scripts": {
    "build": "tsc",
    "build-to-publish": "tsc --outDir dist",
    "create-nightwatch": "tsc && node dev-run.js",
